   
   
<%=  form_for(@user, url: '/complete') do |f|%>
  <%= f.label :username %>
  <%= f.text_field :username %>
  <br>
  <br>  
<%= f.label "Please select an account type:" %>
  <br>
  <%= f.label "CLIENT:" %>
  <%= f.check_box :client, {}, true, false %> 
  <%= f.label "DESIGNER:" %>
  <%= f.check_box :designer, {}, true, false %>